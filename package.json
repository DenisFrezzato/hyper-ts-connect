  "keywords": [
    "typescript",
    "connect",
    "middleware",
    "fp-ts",
    "io-ts",
    "functional-programming",
    "hyper"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run build && npm run test"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write",
      "npm run lint"
    ]
  },
  "dependencies": {
    "qs": "^6.10.1"
  }
