  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/connect": "^3.4.34",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.0.27",
    "@types/qs": "^6.9.7",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "body-parser": "^1.19.0",
    "connect": "^3.7.0",
    "docs-ts": "^0.6.10",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "fp-ts": "^2.8.1",
    "fp-ts-contrib": "^0.1.17",
    "fp-ts-routing": "^0.5.4",
    "husky": "^4.3.8",
    "hyper-ts": "^0.7.4",
    "io-ts": "^2.2.9",
    "io-ts-types": "^0.5.9",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "prettier": "^2.0.5",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^8.10.2"
  },
  "peerDependencies": {
    "connect": "^3.7.0",
    "fp-ts": "^2.8.1",
    "fp-ts-contrib": "^0.1.17",
    "hyper-ts": "^0.7.4"
  },
  "tags": [
    "typescript",
    "connect",
    "middleware",
    "fp-ts",
    "io-ts",
    "functional-programming",
